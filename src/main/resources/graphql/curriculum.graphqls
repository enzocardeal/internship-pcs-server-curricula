scalar Date

type Curriculum{
    id: ID!
    name: String!
    lastName: String!
    degreeCourse: String!
    graduationYear: Date!
    pastExperiences: [Experience]!
    certificates: [Activity]!
}

type Experience{
    company: String!
    role: String!
    description: String!
    startedAt: Date!
    endedAt: Date!
}

type Activity{
    name: String!
    description: String!
    completedAt: Date!
    expiresAt: Date!
}

input ExperienceInput{
    company: String!
    role: String!
    description: String!
    startedAt: Date!
    endedAt: Date!
}

input ActivityInput{
    name: String!
    description: String!
    completedAt: Date!
    expiresAt: Date!
}

input CreateCurriculumInput{
    name: String!
    lastName: String!
    degreeCourse: String!
    graduationYear: Date!
    pastExperiences: [ExperienceInput]!
    certificates: [ActivityInput]!
}

input UpdateCurriculumInput{
    name: String
    lastName: String
    degreeCourse: String
    graduationYear: Date
    pastExperiences: [ExperienceInput]
    certificates: [ActivityInput]
}
